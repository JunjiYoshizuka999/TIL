// https://paiza.jp/works/mondai/c_rank_level_up_problems/c_rank_simulation_step1

import java.util.*;


public class Main {
    public static void main(String[] args) {
        int n = 10000;

        while (true) {
            if (n % 13 == 0) {
                break;
            }
            
            n += 1;
        }

        System.out.println(n);
    }
}
