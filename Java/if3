//https://paiza.jp/works/mondai/conditions_branch/conditions_branch__simple_boss

import java.util.*;
public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        boolean ans = false;
        for (int i = 0; i < n; i++) {
            int a = sc.nextInt();
            if (a == 0) {
                ans = true;
            }
        }
        
        if (ans) {
            System.out.println("NO");
        } else {
            System.out.println("YES");
        }
    }
}
