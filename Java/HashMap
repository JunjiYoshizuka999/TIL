//辞書を用いた出力の練習問題
// https://paiza.jp/works/mondai/c_rank_level_up_problems/c_rank_dictionary_step1
import java.util.*;
public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        HashMap<String, Integer> sa = new HashMap<>();
        
        for (int i = 0; i < n; i++ ) {
            String s = sc.next();
            int a = sc.nextInt();
            
            sa.put(s, a);
        
        }
        
        System.out.println(sa.get(sc.next()));
    }
}
